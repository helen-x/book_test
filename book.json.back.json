{
    "title": "Test handbook",
    "description": "gitbook 测试",
    "author": "菜刀文",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.2",
    "root": ".",
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "简书": "http://www.jianshu.com/u/ccabae3e72f2"
        }
    },
    "plugins": [
        "-lunr",
        "-search",
        "-highlight",
        "-livereload",
        "search-plus@^0.0.11",
        "splitter@^0.0.8",
       
        "edit-link@^2.0.2",
 
        "tbfed-pagefooter@^0.0.1"
 
     
    ],
    "pluginsConfig": {
      
        "sharing": {
            "weibo": true,
            "facebook": true,
            "twitter": true,
            "google": false,
            "instapaper": false,
            "vk": false,
            "all": [
                "facebook",
                "google",
                "twitter",
                "weibo",
                "instapaper"
            ]
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright © 菜刀文 ",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
       
    
        "edit-link": {
            "base": "https://github.com/helen-x/book_test/edit/master",
            "label": "Edit This Page"
        }
    }
}